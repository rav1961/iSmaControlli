    "identity-obj-proxy": "^3.0.0",
    "jest": "29.7.0",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "sass": "^1.69.5",
    "tailwindcss": "3.3.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2",
    "vite": "5.0.0",
    "vitest": "^0.34.6"
  }
